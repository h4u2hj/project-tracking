{
  "name": "manageprojects",
  "version": "0.0.1",
  "description": "Create and manage new and in-progress projects",
  "keywords": [
    "ui5",
    "openui5",
    "sapui5"
  ],
  "main": "webapp/index.html",
  "devDependencies": {
    "@sap-ux/ui5-middleware-fe-mockserver": "^2.3.17",
    "@sap/ui5-builder-webide-extension": "^1.1.9",
    "@sap/ux-ui5-tooling": "1",
    "@ui5/cli": "^4.0.16",
    "@ui5/middleware-code-coverage": "^2.0.1",
    "mbt": "^1.2.34",
    "ui5-middleware-livereload": "^3.1.4",
    "ui5-middleware-simpleproxy": "^3.5.1",
    "ui5-task-zipper": "^3"
  },
  "scripts": {
    "deploy-config": "npx -p @sap/ux-ui5-tooling fiori add deploy-config cf",
    "build": "ui5 build preload --clean-dest --config ui5-deploy.yaml",
    "build-local": "ui5 build preload --clean-dest",
    "start": "ui5 serve --config ./ui5-test.yaml",
    "testOpa": "ui5 serve --config ./ui5-test.yaml --open test/integration/opaTests.qunit.html?coverage"
  },
  "ui5": {
    "dependencies": [
      "ui5-task-zipper"
    ]
  }
}
